{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cocos-Launch",
            "type": "lua",
            "request": "launch",
            "runtimeType": "Cocos2",
            "localRoot": "${workspaceRoot}",
            "mainFile": "src/main.lua",
            "commandLine": [
                "-workdir ${workspaceRoot}/../",
                "-file src/main.lua",
                "-write-debug-log debug.log",
            ],
            "port": 7003,
            "exePath": "${workspaceRoot}/../simulator/win32/hellolua.exe",
            "printType": 1,
            "printDate": true,
            "preLaunchTask": "kill_exe"
        },
        {
            "name": "Lua51-Launch",
            "type": "lua",
            "request": "launch",
            "runtimeType": "Lua51",
            "localRoot": "${workspaceRoot}",
            "mainFile": "main",
            "port": 7003,
            "printType": 1,
            "printDate": true
        }
       
    ]
}
